{
  "name": "scaffold",
  "description": "scaffold of ejs gulp yarn react",
  "version": "0.0.1",
  "repository": "https://github.com/Caesar73/scaffold",
  "bugs": "https://github.com/koajs/examples/issues",
  "main": "server/app.js",
  "bin": "./node_modules/.bin/",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "debug": "cross-env NODE_ENV=development nodemon --inspect ./bin/www",
    "serve:test": "cross-env NODE_ENV=test nodemon ./bin/www",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text ava --serial",
    "start": "node ./bin/www",
    "html": "node server/html",
    "analyze": "source-map-explorer build/static/js/main.*",
    "react-start": "node scripts/start.js",
    "react-build": "node scripts/build.js",
    "react-test": "node scripts/test.js --env=jsdom",
    "pretest": "jshint .",
    "startbe": "node .",
    "mocha-test": "mocha test/*test.js"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015-node6"
    ],
    "plugins": [
      "espower",
      "transform-runtime"
    ]
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "debug": "^3.0.0",
    "del": "*",
    "ejs": "^2.5.6",
    "fs-promise": "^2.0.3",
    "ioredis": "^3.1.4",
    "kcors": "^2.2.1",
    "koa": "^2.2.0",
    "koa-basic-auth": "^2.0.0",
    "koa-body": "^2.0.1",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-csrf": "^3.0.6",
    "koa-generic-session": "^2.0.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.2.1",
    "koa-logger": "^3.0.0",
    "koa-multer": "^1.0.2",
    "koa-passport": "^3.0.0-rc.2",
    "koa-redis": "^3.0.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.2.0",
    "koa-session": "^5.0.0",
    "koa-static": "^3.0.0",
    "koa-views": "^6.0.2",
    "koa-xtpl": "2.1.0",
    "lodash": "^4.17.4",
    "markdown-it": "^8.3.2",
    "mongoose": "^4.11.7",
    "node-glob": "*",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-qq": "0.0.3",
    "passport-weibo": "^0.1.2",
    "qiniu": "^7.0.6",
    "streaming-json-stringify": "^3.1.0",
    "supertest": "^3.0.0",
    "swig": "^1.4.2",
    "trek-captcha": "^0.3.0",
    "xss": "^0.3.3",
    "xtemplate": "^4.6.1",
    "xtpl": "^3.3.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "ava-spec": "^1.0.1",
    "babel-plugin-espower": "^2.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^2.11.15",
    "cross-env": "^5.0.5",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "*",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "nyc": "^11.1.0",
    "should": "^3.3.2",
    "sinon": "^3.2.1",
    "superkoa": "^1.0.3",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">= 7.6"
  },
  "license": "MIT",
  "author": "Caesar"
}
